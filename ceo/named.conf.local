// Zona directa
zone "olimpo.test" {
    type slave;
    file "/var/cache/bind/db.olimpo.test";
    masters { 192.168.57.10; };
};

// Zona inversa
zone "57.168.192.in-addr.arpa" {
    type slave;
    file "/var/cache/bind/db.192.168.57";
    masters { 192.168.57.10; };
};
